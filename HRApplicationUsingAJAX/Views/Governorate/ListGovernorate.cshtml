@model IEnumerable<HRApplicationUsingAJAX.Models.Governorate>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            <button type="button" class="btn btn-warning" onclick="EditGovernorate(@item.ID)">Edit</button><span>|</span>
            <button type="button" class="btn btn-danger" onclick="DeleteGovernorate(@item.ID)">Delete</button>
        </td>
    </tr>
}

</table>
